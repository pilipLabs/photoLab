{
    "name": "piliplabs/photolab",
    "type": "library",
    "description": "A PHP 5.3 photo gallery",
    "keywords": [],
    "homepage":
    "https://github.com/pilipLabs/photoLab",
    "license": "Apache 2.0",
    "minimum-stability": "dev",
    "authors": [{
        "name": "pilip Labs",
        "email": "labs@pilip.net",
        "homepage": "http://labs.pilip.net/"
    }],
    "require": {
        "silex/silex": "*",
        "twig/twig": ">=1.8,<2.0-dev",
        "symfony/twig-bridge": "2.1.*",
        "grom/silex-service-provider": "*",
        "leafo/lessphp": "*",
        "twitter/bootstrap": "dev-master",
        "fate/silex-extensions": "dev-master",
        "kriswallsmith/assetic": "dev-master",
        "symfony/finder": "dev-master"
    },
    "repositories" : [{
        "type" : "package",
        "package" : {
            "name" : "twitter/bootstrap",
            "version" : "master",
            "source" : {
                "url": "https://github.com/twitter/bootstrap.git",
                "type": "git",
                "reference": "master"
            },
            "dist": {
                "url": "https://github.com/twitter/bootstrap/zipball/master",
                "type": "zip"
            }
        }
    }],
    "autoload":
    {
       "psr-0": {
           "photo_lab":  "src"
       }
    }

}
